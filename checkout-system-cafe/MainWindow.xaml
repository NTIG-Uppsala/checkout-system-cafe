<Window x:Class="checkout_system_cafe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:checkout_system_cafe"
        mc:Ignorable="d"
        Title="Checkout-cafe" Height="1024" Width="768">
    <Grid RenderTransformOrigin="0.45,0.527">
        <Label x:Name="totalPrice" Content="0 kr" HorizontalAlignment="Left" Margin="370,221,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.489,0.54"/>
        <Button x:Name="coffee" Content="Kaffe" HorizontalAlignment="Left" Margin="334,224,0,0" VerticalAlignment="Top" Click="CoffeeClick"/>
        <Button x:Name="reset" Content="Nollställ" HorizontalAlignment="Left" Margin="334,250,0,0" VerticalAlignment="Top" Click="ResetClick"/>
    </Grid>
</Window>
